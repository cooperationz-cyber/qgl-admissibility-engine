boundary TSPProblem {
    cities, distances, path
}

domain Cities {
    A, B, C, D, E
}

domain Distances {
    AB_plus_BA, AC_plus_CA, AD_plus_DA, AE_plus_EA,
    BC_plus_CB, BD_plus_DB, BE_plus_EB,
    CD_plus_DC, CE_plus_EC,
    DE_plus_ED
}

domain PathChoices {
    from_A_B_plus_from_A_C_plus_from_A_D_plus_from_A_E,
    from_B_A_plus_from_B_C_plus_from_B_D_plus_from_B_E,
    from_C_A_plus_from_C_B_plus_from_C_D_plus_from_C_E,
    from_D_A_plus_from_D_B_plus_from_D_C_plus_from_D_E,
    from_E_A_plus_from_E_B_plus_from_E_C_plus_from_E_D
}

domain HamiltonianConstraint {
    visited_once_A, visited_once_B, visited_once_C,
    visited_once_D, visited_once_E
}